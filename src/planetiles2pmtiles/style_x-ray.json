    },
    {
      "id": "labels-all",
      "type": "symbol",
      "source": "n5000-source",
      "source-layer": "N5000_Stedsnavn_tekstplassering",
      "layout": {
        "text-field": [
          "get",
          "TextString"
        ],
        "text-font": [
          "Open Sans Regular",
          "Arial Unicode MS Regular"
        ],
        "text-size": 10,
        "text-max-width": 8,
        "text-anchor": "top",
        "text-padding": 2,
        "text-allow-overlap": true
      },
      "paint": {
        "text-color": "#e0e0e0",
        "text-halo-width": 0
      }
    }
  ]
}